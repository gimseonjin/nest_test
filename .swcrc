{
    "sourceMaps": "inline",
    "jsc": {
        "parser": {
              "syntax": "typescript",
              "tsx": false,
              "decorators": true,
              "dynamicImport": false
            },
           "transform": {
              "legacyDecorator": true,
              "decoratorMetadata": true
            },
        "target": "es2021",
        "paths": {
          "@/*": ["src/*"],
          "!/*": ["test/*"]
        },
        "baseUrl": "./"
    },
     "module": {
        "type": "commonjs",
        "strict": false,
        "strictMode": true,
        "noInterop": false
      }
}